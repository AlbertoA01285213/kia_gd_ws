            <geometry>
                <mesh filename="./meshes/J1_m_ascii.STL" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <link name="J2">
        <visual>
            <origin rpy="1.57 0.0 1.57" xyz="0.026975 0.0 0"/>
            <geometry>
