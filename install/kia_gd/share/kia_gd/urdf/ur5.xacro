<?xml version="1.0"?>
<robot name="UR5">

    <link name="Prueba">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.2"/>
            </geometry>
        </visual>
    </link>


    <link name="Base">
        <visual>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://home/alberto/Documentskia_gd_ws/src/kia_gd/meshes/Base_m_ascii.STL" scale="1 1 1"/>
            </geometry>
        </visual>
    </link>

    <link name="J1">
        <visual>
