            </geometry>
        </visual>
    </link>

    <link name="world"/>

    <joint name="world_to_base" type="fixed">
        <parent link="world"/>
        <child link="Base"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <joint name="world_to_prueba" type="fixed">
        <parent link="world"/>
        <child link="Prueba"/>
        <origin xyz="0 0 1" rpy="0 0 0"/> 
    </joint>

    <joint name="Base_J1" type="continuous">
        <parent link = "Base"/>
        <child link = "J1"/>
        <origin xyz="0 0.0 0.0" rpy="0 0 0" />
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="J1_J2" type="continuous">
        <parent link = "J1"/>
        <child link = "J2"/>
        <origin xyz="0 0.0 0.0" rpy="0 0 3.141592" />
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="J2_J3" type="continuous">
        <parent link = "J2"/>
        <child link = "J3"/>
        <origin xyz="0 0.075 -0.02" rpy="0 0 3.141592" />
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="J3_J4" type="continuous">
        <parent link = "J3"/>
        <child link = "J4"/>
        <origin xyz="0 0.075 -0.02" rpy="0 0 3.141592" />
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="J4_J5" type="continuous">
